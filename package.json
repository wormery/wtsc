{
  "name": "@wormery/wtsc",
  "version": "2.0.0-bate.1",
  "description": "Generate vue style value using TS",
  "main": "./lib/wtsc.common.js",
  "module": "lib/wtsc.esm.js",
  "unpkg": "lib/wtsc.umd.js",
  "jsdelivr": "lib/wtsc.iife.js",
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/wormery/wtsc"
  },
  "scripts": {
    "build": "pnpm build:all",
    "build:all": "pnpm rollup -c",
    "format": "prettier --write \"./**/*.{js,jsx,json,yaml}\"",
    "test": "mocha",
    "unit": "pnpm ts-mocha -p tsconfig.json test/**/*.spec.ts",
    "release:minor-alpha": "pnpm build && pnpm standard-version --release-as minor --prerelease alpha",
    "release:major-bate": "pnpm build && pnpm standard-version --release-as major --prerelease bate",
    "release:bate": "pnpm build && pnpm standard-version --prerelease bate",
    "release": "pnpm build && pnpm standard-version"
  },
  "author": "meke",
  "license": "MIT",
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "style",
    "styleValue"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@types/bluebird": "*",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "mocha": "^9.2.0",
    "prettier": "^2.5.1",
    "rollup": "^2.63.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-mocha": "^9.0.2",
    "ts-toolbelt": "^9.6.0",
    "typescript": "~4.4.0"
  },
  "dependencies": {
    "@wormery/utils": "1.0.1"
  }
}
